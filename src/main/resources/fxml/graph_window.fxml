            <children>
               <ChoiceBox fx:id="chartChoiceBox" layoutX="50.0" layoutY="45.0" prefWidth="150.0" stylesheets="@style.css">
                  <styleClass>
                     <String fx:value="filterButtonColor" />
                     <String fx:value="choice-box" />
                  </styleClass>
               </ChoiceBox>
               <Label layoutX="88.0" layoutY="14.0" text="Chart type" textFill="WHITE">
                  <font>
                     <Font name="System Bold" size="15.0" />
                  </font>
               </Label>
            </children>
         </AnchorPane>
         <AnchorPane fx:id="graphsDataPane" layoutX="339.0" layoutY="14.0" maxHeight="117.0" maxWidth="250.0" prefHeight="83.0" prefWidth="250.0" styleClass="sideBarColor" stylesheets="@style.css">

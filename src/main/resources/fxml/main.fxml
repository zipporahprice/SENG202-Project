<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<StackPane fx:id="mainWindow" minHeight="400" minWidth="600" prefHeight="774.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team0.gui.MainController">
   <children>
      <WebView fx:id="webView" prefHeight="-1.0" prefWidth="-1.0">
         <StackPane.margin>
            <Insets />
         </StackPane.margin></WebView>
      <Region maxWidth="-Infinity" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: linear-gradient(to right,rgba(46,54,255,1) 0%, rgba(99,121,241,1) 0%, rgba(126,155,234,1) 9%, rgba(138,170,231,1) 13%, rgba(146,180,229,1) 17%, rgba(158,196,226,1) 23%, rgba(170,211,223,1) 31%);;" StackPane.alignment="CENTER_LEFT">
         <effect>
            <BoxBlur height="0.0" width="255.0" />
         </effect>
      </Region>

      <AnchorPane fx:id="menuDisplayPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.0" prefWidth="0.0" StackPane.alignment="TOP_LEFT">
      </AnchorPane>

      <AnchorPane maxHeight="-Infinity" maxWidth="318.0" prefHeight="573.0" prefWidth="318.0" styleClass="sideBarColor" stylesheets="@style.css" StackPane.alignment="TOP_LEFT">
         <children>
            <ImageView fitHeight="43.0" fitWidth="187.0" layoutX="32.0" layoutY="26.0">
               <image>
                  <Image url="@images/SafeTripLogoLarge.png" />
               </image>
            </ImageView>
            <Button fx:id="routingButton" layoutX="9.0" layoutY="79.0" mnemonicParsing="false" onAction="#toggleMenuDisplay" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css" userData="routing">
               <graphic>
                  <ImageView fitHeight="34.0" fitWidth="200.0">
                     <image>
                        <Image url="@images/plan_route.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="filterDataButton" layoutX="9.0" layoutY="159.0" mnemonicParsing="false" onAction="#toggleMenuDisplay" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css" userData="filtering">
               <graphic>
                  <ImageView fitHeight="34.0" fitWidth="200.0">
                     <image>
                        <Image url="@images/filter_data.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="importExportButton" layoutX="9.0" layoutY="239.0" mnemonicParsing="false" onAction="#toggleMenuDisplay" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css" userData="import">
               <graphic>
                  <ImageView fitHeight="36.0" fitWidth="250.0">
                     <image>
                        <Image url="@images/import_export.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="settingsButton" layoutX="9.0" layoutY="319.0" mnemonicParsing="false" onAction="#toggleMenuDisplay" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css" userData="settings">
               <graphic>
                  <ImageView fitHeight="44.0" fitWidth="200.0">
                     <image>
                        <Image url="@images/settings.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="userHelpButton" layoutX="9.0" layoutY="399.0" mnemonicParsing="false" onAction="#toggleMenuDisplay" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css" userData="help">
               <graphic>
                  <ImageView fitHeight="39.0" fitWidth="200.0">
                     <image>
                        <Image url="@images/user_help.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="graphsButton" layoutX="9.0" layoutY="479.0" mnemonicParsing="false" onAction="#loadGraphs" prefHeight="67.0" prefWidth="300.0" styleClass="menuButtonColor" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="160.0">
                     <image>
                        <Image url="@images/graphs_icon.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
         <StackPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
      </AnchorPane>
      <Button fx:id="quitButton" onAction="#quitApp" prefHeight="67.0" prefWidth="110.0" styleClass="menuButtonColor" stylesheets="@style.css" StackPane.alignment="BOTTOM_LEFT">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="67.0">
               <image>
                  <Image url="@images/quit_text.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>

      <StackPane fx:id="loadingScreen" prefHeight="150.0" prefWidth="200.0" visible="false">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" styleClass="red-pane" stylesheets="@style.css" />
            <ProgressBar fx:id="progressBar" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="300.0" progress="0.0" />
            <Label fx:id="loadingPercentageLabel" layoutX="50" layoutY="50" text="0%">
               <font>
                  <Font name="Open Sans Bold" size="20.0" />
               </font></Label>
            <ImageView fitHeight="150.0" fitWidth="300.0" preserveRatio="true" StackPane.alignment="TOP_CENTER">
               <image>
                  <Image url="@images/SafeTripLogoLarge.png" />
               </image>
               <StackPane.margin>
                  <Insets top="250.0" />
               </StackPane.margin>
            </ImageView>
         </children>
      </StackPane>
   </children>
</StackPane>
